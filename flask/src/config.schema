{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "webapp": {
            "type": "object",
            "properties": {
                "timezone": {
                    "type": "object",
                    "properties": {
                        "offset": {
                            "type": "string"
