        "liveness": {
            "type": "object",
            "properties": {
                "file": {
                    "type": "object",
                    "properties": {
                        "scheduler": {
                            "type": "string"
                        },
                        "valve_control": {
                            "type": "string"
                        },
                        "flow_notify": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "flow_notify",
                        "scheduler",
                        "valve_control"
                    ]
                }
            },
            "required": [
                "file"
            ]
        }
    },
    "required": [
        "control",
        "flow",
        "fluent",
        "influxdb",
        "liveness",
        "slack",
        "weather",
        "webapp"
    ]
