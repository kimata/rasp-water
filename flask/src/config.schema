                                    "properties": {
                                        "before_hour": {
                                            "type": "integer"
                                        },
                                        "sum": {
                                            "type": "integer"
                                        }
                                    },
                                    "required": [
                                        "before_hour",
                                        "sum"
                                    ]
                                }
                            },
                            "required": [
                                "point",
                                "threshold",
                                "yahoo"
                            ]
                        },
                        "sensor": {
                            "type": "object",
                            "properties": {
                                "hostname": {
                                    "type": "string"
                                },
                                "measure": {
                                    "type": "string"
                                },
                                "threshold": {
                                    "type": "object",
                                    "properties": {
                                        "sum": {
                                            "type": "integer"
                                        }
                                    },
                                    "required": [
                                        "sum"
                                    ]
                                }
                            },
                            "required": [
                                "hostname",
                                "measure",
                                "threshold"
                            ]
                        }
                    },
                    "required": [
                        "forecast",
                        "sensor"
                    ]
                }
            },
            "required": [
                "rain_fall"
            ]
        },
        "slack": {
            "type": "object",
            "properties": {
                "bot_token": {
                    "type": "string"
                },
                "from": {
                    "type": "string"
                },
                "error": {
                    "type": "object",
                    "properties": {
                        "channel": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "id": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "id",
                                "name"
                            ]
