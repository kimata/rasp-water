                        "name": {
                            "type": "string"
                        },
                        "zone": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "name",
                        "offset",
                        "zone"
                    ]
                },
                "static_dir_path": {
                    "type": "string"
                },
                "data": {
                    "type": "object",
                    "properties": {
                        "schedule_file_path": {
                            "type": "string"
                        },
                        "log_file_path": {
                            "type": "string"
                        },
                        "stat_dir_path": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "log_file_path",
                        "schedule_file_path",
                        "stat_dir_path"
                    ]
                }
            },
            "required": [
                "data",
                "static_dir_path",
