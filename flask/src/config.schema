        "control": {
            "type": "object",
            "properties": {
                "gpio": {
                    "type": "integer"
                }
            },
            "required": [
                "gpio"
            ]
        },
        "flow": {
            "type": "object",
            "properties": {
                "sensor": {
                    "type": "object",
                    "properties": {
                        "scale": {
                            "type": "object",
                            "properties": {
                                "max": {
                                    "type": "integer"
                                }
                            },
                            "required": [
                                "max"
                            ]
                        },
                        "adc": {
                            "type": "object",
                            "properties": {
                                "scale_file": {
                                    "type": "string"
                                },
                                "scale_value": {
                                    "type": "integer"
                                },
                                "value_file": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "scale_file",
                                "scale_value",
                                "value_file"
                            ]
                        }
                    },
                    "required": [
                        "adc",
                        "scale"
                    ]
                },
                "offset": {
                    "type": "number"
                }
            },
            "required": [
                "offset",
                "sensor"
            ]
        },
        "fluent": {
            "type": "object",
            "properties": {
                "host": {
                    "type": "string"
                },
                "data": {
                    "type": "object",
