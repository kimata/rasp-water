        "influxdb": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "org": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                },
                "bucket": {
                    "type": "string"
                }
            },
            "required": [
                "bucket",
                "org",
                "token",
                "url"
            ]
        },
        "weather": {
            "type": "object",
            "properties": {
                "rain_fall": {
                    "type": "object",
                    "properties": {
                        "forecast": {
                            "type": "object",
                            "properties": {
                                "yahoo": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
